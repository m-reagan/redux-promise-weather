{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": 6
    },
    "env": {
        "node":true,
        "es6": true,
        "browser": true
    },
    "rules": {
        "react/jsx-filename-extension":"off",
        "linebreak-style":0,
        "react/prefer-stateless-function":0,
        "comma-dangle":0
    }
}